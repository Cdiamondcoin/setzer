#!/usr/bin/env bash
set -e
bitbay=$(setzer price bitbay)
bitfinex=$(setzer price bitfinex)
bittrex=$(setzer price bittrex)
#btce=$(setzer price btce)
cex=$(setzer price cex)
coinbase=$(setzer price coinbase)
coinmarketcap=$(setzer price coinmarketcap)
cryptocompare=$(setzer price cryptocompare)
etherscan=$(setzer price etherscan)
gdax=$(setzer price gdax)
gemini=$(setzer price gemini)
hitbtc=$(setzer price hitbtc)
kraken=$(setzer price kraken)
livecoin=$(setzer price livecoin)
poloniex=$(setzer price poloniex)
yobit=$(setzer price yobit)
echo "Bitbay:         $bitbay"
echo "Bitfinex:       $bitfinex"
echo "Bittrex:        $bittrex"
#echo "BTC-E:          $btce"
echo "Cex:            $cex"
echo "Coinbase:       $coinbase"
echo "CoinMarketCap:  $coinmarketcap"
echo "CryptoCompare:  $cryptocompare"
echo "Etherscan:      $etherscan"
echo "GDAX:           $gdax"
echo "Gemini:         $gemini"
echo "HitBTC:         $hitbtc"
echo "Kraken:         $kraken"
echo "Livecoin:       $livecoin"
echo "Poloniex:       $poloniex"
echo "Yobit:          $yobit"